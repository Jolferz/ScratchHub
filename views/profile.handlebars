<div class="col-sm-12 main center-me" >
        
        <!-- resumt -->
        <div class="panel panel-default">
               <div class="panel-heading resume-heading">
                  <div class="row">
                     <div class="col-lg-12">
                        <div class="col-xs-12 col-sm-4">
                           <figure>
                              <img class="img-circle img-responsive" alt="" src="http://seniorsfs.global2.vic.edu.au/files/2013/05/realmario-12fstdk-300x300.jpg">
                           </figure>
                        </div>
                        <div class="col-xs-12 col-sm-8 vertical-center">
                           <ul class="list-group">
                              <li class="list-group-item"><strong>Username: </strong>{{ username }}</li>
                              <li class="list-group-item"><strong>Real name: </strong>{{ name }}</li>
                              <li class="list-group-item"></i><strong>Email: </strong>{{ email }}</li>
                           </ul>
                        </div>
                     </div>
                  </div>
               </div>
               <div class="bs-callout bs-callout-danger">
                  <h4>About Me</h4>
                  <p>
					  {{ aboutMe }}
                  </p>
               </div>
               <div class="bs-callout bs-callout-danger">
                  <h4>Interests</h4>
                  <p>
					  {{ interests }}
                  </p>
               </div>
				<div class="bs-callout bs-callout-danger">
					<h4>Projects</h4>
						<ul class="list-group">
						{{#each projects}}
							<a class="list-group-item inactive-link" href="/project/{{ this.name }}">
								<h4 class="list-group-item-heading">{{ this.name }}</h4>
								<p class="list-group-item-text">
									{{ this.description }}
								</p>
							</a>
						{{/each}}			
					</ul>
				</div>
               
            </div>
         </div>
        <!-- resume -->