<div class="col-sm-12 main center-me" >
    <!-- resumt -->
    <div class="panel panel-default">
        <div class="panel-heading resume-heading">
            <div class="row">
                <div class="col-lg-12">
                    <div class="col-md-10 col-lg-4 col-md-offset-1">
                        {{{ iframe }}}
                    </div>
                    <div class="col-md-12  vertical-center ">
                        <ul class="list-group">
                            <li class="list-group-item"><strong>Name:</strong> {{ name }}</li>
                            <li class="list-group-item"><strong>Category:</strong> {{ category }}</li>
                            <li class="list-group-item"><strong>Author:</strong> {{ author }}</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="bs-callout bs-callout-danger project-description">
            <h4>Description</h4>
            <p>
                {{ description }}
            </p>
        </div>               
    </div>
</div>


